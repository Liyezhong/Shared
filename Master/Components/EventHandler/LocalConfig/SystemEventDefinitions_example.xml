<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE colorado_err_definitionn>
<colorado_event_definitions version="1.0" >
    <event_definitions language="english">
    <!-- Description of the event attribute "type" -->
    <!-- 
    English event definition section is the default.
    It shall exist in all device versions and configurations.
    Each event has an attribute "type" in this (and only) section.
    This attribute is used by Event Handler for the runtime Event 
    processing. Following values are allowed for "type" attribute:
        : fatalerror
        : error
        : warning
        : info
    -->
        <event_main_group key="app" ID="0x01">
            <event_sub_group key="app_ctrl" ID="0x01">
                <event ID="0x0001"
                        type="info"
                        shorttext="Application start"
                        text="Application start."/>
                <event ID="0x0002"
                        type="info"
                        shorttext="GUI start"
                        text="Gui start."/>
                <event ID="0x0003"
                        type="info"
                        shorttext="DCL start"
                        text="DCL start."/>
                <event ID="0x0004"
                        type="info"
                        shorttext="DCL-Configuration start"
                        text="DCL-Configuration start."/>
                <event ID="0x0005"
                        type="info"
                        shorttext="DCL-Diagnsotic service start"
                        text="DCL-Diagnsotic service start."/>

            </event_sub_group>
        </event_main_group>
        <event_main_group key="gui" ID="0x02">
        </event_main_group>
        <event_main_group key="dcl" ID="0x03">
