FormatVersion=5.0
Title="Symbol File for Colorado"


{ENUMS}

enum EnumNodeStates(
  0 = "UNDEFINED", 
  1 = "BOOTING", 
  2 = "STARTUP", 
  3 = "IDENTIFY", 
  4 = "CONFIGURE", 
  5 = "NORMAL", 
  6 = "DIAGNOSE", 
  7 = "SHUTDOWN", 
  8 = "STANDBY",
  9 = "UPDATE")

enum EnumPowerStatus(
  0 = "GOOD", 
  1 = "WARNING", 
  2 = "FAILURE", 
  9 = "UNKNOWN")

enum EnumStopStatus(
  0 = "INACTIVE", 
  1 = "STOPPED BY SLAVE", 
  2 = "STOPPED BY MASTER", 
  3 = "STOPPED BY BOTH")

enum EnumStatSelect(
  0 = "NO DATA", 
  1 = "RUNTIME", 
  2 = "INTERVAL", 
  3 = "RESERVED")

enum EnumBoolean(
  0 = "FALSE", 
  1 = "TRUE")

enum EnumBootLoaderMode(
  0 = "START FIRMWARE",
  1 = "START UPDATE")
  
enum EnumBootLoaderHeader(
  0 = "READY",
  1 = "IMAGE TOO LARGE",
  2 = "OFFSET TOO LARGE")
  
enum EnumBootLoaderTrailer(
  0 = "SUCCESSFUL",
  1 = "CHECKSUM INVALID",
  2 = "LENGTH INVALID")

enum EnumDoState (
  0 = "STATE_IDLE",
  1 = "STATE_DELAY",
  2 = "STATE_DURATION",
  3 = "STATE_OUTPUT",
  4 = "STATE_NOTIFY",
  5 = "STATE_ABORT")

